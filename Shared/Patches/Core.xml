                    <li Class="StockGenerator_Tag">
                        <tradeTag>AmmoIndustrial</tradeTag>
                        <thingDefCountRange>0~2</thingDefCountRange>
                        <countRange>50~100</countRange>
                    </li>
                </value>
            </li>
        </operations>

    </Operation>

</Patch>