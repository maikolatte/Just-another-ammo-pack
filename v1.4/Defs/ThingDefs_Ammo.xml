<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!--277 Fury-->
    <ThingDef ParentName="AmmunitionBase" Name="JAAP_277FuryBase" Abstract="True">
        <soundInteract>Interact_Rifle</soundInteract>
        <techLevel>Industrial</techLevel>
        <tradeTags>
            <li>AmmoIndustrial</li>
        </tradeTags>
        <comps>
            <li Class="CompProperties_Explosive">
                <explosiveRadius>2.0</explosiveRadius>
                <explosiveDamageType>Burn</explosiveDamageType>
                <requiredDamageTypeToExplode>Flame</requiredDamageTypeToExplode>
                <startWickHitPointsPercent>0.7</startWickHitPointsPercent>
                <wickTicks>
                    <min>30</min>
                    <max>60</max>
                </wickTicks>
            </li>
        </comps>
        <statBases>
            <MarketValue>0.55</MarketValue>
            <Mass>0.01</Mass>
        </statBases>
    </ThingDef>
    <ThingDef ParentName="JAAP_277FuryBase">
        <defName>JAAP_277FuryFMJ</defName>
        <label>.277 Fury (fmj)</label>
        <description>.277 Fury ammunition that uses an explosive primer that will fire projectiles at incredible speeds.

            Damage: 18
            Armor penetration: 18%
            Stopping power: 1</description>
        <graphicData>
            <texPath>Ammo/Ammo277FuryFMJ</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <uiIconPath>Ammo/Ammo277FuryFMJ/277FuryFMJ_a</uiIconPath>
        <modExtensions>
            <li MayRequire="LimeTreeSnake.Ammunition" Class="Ammunition.DefModExtensions.AmmunitionExtension">
                <bulletDef>JAAP_277FuryProjectile</bulletDef>
            </li>
        </modExtensions>
    </ThingDef>

