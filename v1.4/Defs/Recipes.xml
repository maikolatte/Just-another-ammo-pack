<Defs>
    <!--JAAP_227FuryFMJ -->
    <RecipeDef Name="JAAP_277FuryBase_Recipe" Abstract="True">
        <description>Make bullets that go pew.</description>
        <jobString>Making bullets.</jobString>
        <workAmount>1200</workAmount>
        <workSkill>Crafting</workSkill>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Smith</effectWorking>
        <soundWorking>Recipe_Smith</soundWorking>
        <recipeUsers>
            <li>LTS_AmmunitionBench</li>
        </recipeUsers>
        <allowMixingIngredients>false</allowMixingIngredients>
        <skillRequirements>
            <Crafting>6</Crafting>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                        <li MayRequire="Argon.VMEu">VMEu_Lead</li>
                    </thingDefs>
                </filter>
                <count>50</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Steel</li>
                <li MayRequire="Argon.VMEu">VMEu_Lead</li>
            </thingDefs>
        </fixedIngredientFilter>
    </RecipeDef>
    <RecipeDef ParentName="JAAP_277FuryBase_Recipe">
        <defName>Make_AmmoJAAP_277FuryFMJ</defName>
        <label>make .277Fury FMJ ammo</label>
        <products>
            <JAAP_277FuryFMJ>100</JAAP_277FuryFMJ>
        </products>
    </RecipeDef>

